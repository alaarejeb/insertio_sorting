ALGORITHM algorithm_insertion
VAR
    tab : ARRAY_OF INTEGER[n];
    i:INTEGER
    j:INTEGER
    temp:INTEGER
BEGIN
    FOR i FROM 0 TO n-1 STEP 1  DO
            j:=i
            WHILE (j>0 && (t[j]<t[j-1])) DO
                temp:=tab[j]
                tab[j]:=tab[j-1]
                tab[j-1]:=temp
                j:=j-1
            END_WHILE
    END_FOR
END
